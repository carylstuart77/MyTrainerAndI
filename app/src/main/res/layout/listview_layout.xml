<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="edu.cnm.deepdive.mytrainerandi.MainActivity"
  tools:layout_editor_absoluteY="81dp"
  tools:layout_editor_absoluteX="0dp">

  <Button
    android:id="@+id/dateButton"
    android:textStyle="bold"
    android:layout_width="162dp"
    android:layout_height="89dp"
    android:background="@drawable/calendar"
    android:text="Pick Date"
    android:textAlignment="center"
    android:textColor="@android:color/background_dark"
    tools:layout_editor_absoluteY="7dp"
    android:layout_marginLeft="8dp"
    app:layout_constraintLeft_toLeftOf="parent"/>
  <TextView
    android:id="@+id/textView3"
    android:layout_width="90dp"
    android:layout_height="24dp"
    android:text="Muscle"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    app:layout_constraintLeft_toLeftOf="parent"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginTop="24dp"
    app:layout_constraintVertical_bias="0.502"/>
  <TextView
    android:id="@+id/textView4"
    android:layout_width="119dp"
    android:layout_height="27dp"
    android:text="Exercise"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintRight_toLeftOf="@+id/guideline2"
    app:layout_constraintLeft_toRightOf="@+id/textView3"
    app:layout_constraintHorizontal_bias="0.0"
    android:layout_marginTop="24dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintTop_toTopOf="parent"/>
  <TextView
    android:id="@+id/textView6"
    android:layout_width="58dp"
    android:layout_height="34dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginRight="8dp"
    android:text="Reps"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintRight_toLeftOf="@+id/textView12"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.683"/>
  <TextView
    android:id="@+id/textView7"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    tools:layout_editor_absoluteY="96dp"
    tools:layout_editor_absoluteX="221dp"/>

  <android.support.constraint.Guideline
    android:id="@+id/guideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_end="285dp"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="126dp"/>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline2"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.57"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="235dp"/>
  <ListView
    android:id="@+id/listViewDay"
    android:layout_width="395dp"
    android:layout_height="409dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginLeft="0dp"
    app:layout_constraintLeft_toLeftOf="parent">

  </ListView>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline3"
    android:orientation="vertical"
    app:layout_constraintGuide_end="104dp"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="307dp"/>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline4"
    android:orientation="vertical"
    app:layout_constraintGuide_end="45dp"
    tools:layout_editor_absoluteY="0dp"
    tools:layout_editor_absoluteX="366dp"/>
  <TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Sets"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintTop_toTopOf="parent"
    android:layout_marginTop="24dp"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintVertical_bias="0.489"
    app:layout_constraintRight_toLeftOf="@+id/textView6"
    android:layout_marginRight="0dp"
    android:layout_marginEnd="8dp"/>
  <TextView
    android:id="@+id/textView12"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="24dp"
    android:layout_marginBottom="8dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="8dp"
    android:text="LBS"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintVertical_bias="0.675"/>


</android.support.constraint.ConstraintLayout>

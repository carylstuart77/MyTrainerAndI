<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="edu.cnm.deepdive.mytrainerandi.MainActivity"
  tools:layout_editor_absoluteY="81dp"
  tools:layout_editor_absoluteX="0dp">

  <Button
    android:id="@+id/dateButton"
    android:textStyle="bold"
    android:layout_width="162dp"
    android:layout_height="89dp"
    android:background="@drawable/calendar"
    android:text="Pick Date"
    android:textAlignment="center"
    android:textColor="@android:color/background_dark"
    tools:layout_editor_absoluteY="7dp"
    tools:layout_editor_absoluteX="111dp"/>
  <TextView
    android:id="@+id/textView3"
    android:layout_width="90dp"
    android:layout_height="24dp"
    android:text="Muscle"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    android:layout_marginStart="16dp"
    tools:layout_editor_absoluteY="102dp"
    app:layout_constraintRight_toLeftOf="@+id/guideline"
    android:layout_marginRight="-1dp"
    />
  <TextView
    android:id="@+id/textView4"
    android:layout_width="119dp"
    android:layout_height="27dp"
    android:layout_marginRight="-11dp"
    android:text="Exercise"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintRight_toLeftOf="@+id/guideline2"
    tools:layout_editor_absoluteY="102dp"
    />
  <TextView
    android:id="@+id/textView6"
    android:layout_width="60dp"
    android:layout_height="28dp"
    android:layout_marginBottom="-1dp"
    android:layout_marginRight="0dp"
    android:text="Reps"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintRight_toLeftOf="@+id/guideline4"
    tools:layout_editor_absoluteY="100dp"
    tools:layout_editor_absoluteX="279dp"/>
  <TextView
    android:id="@+id/textView7"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    tools:layout_editor_absoluteY="96dp"
    tools:layout_editor_absoluteX="221dp"/>

  <android.support.constraint.Guideline
    android:id="@+id/guideline"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:layout_constraintGuide_end="285dp"/>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline2"
    android:orientation="vertical"
    app:layout_constraintGuide_percent="0.57"/>
  <ListView
    android:id="@+id/listViewDay"
    android:layout_width="390dp"
    android:layout_height="409dp"
    app:layout_constraintHorizontal_bias="1.0"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    tools:layout_editor_absoluteY="128dp">

  </ListView>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline3"
    android:orientation="vertical"
    app:layout_constraintGuide_end="104dp"/>
  <android.support.constraint.Guideline
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/guideline4"
    android:orientation="vertical"
    app:layout_constraintGuide_end="45dp"/>
  <TextView
    android:id="@+id/textView5"
    android:layout_width="52dp"
    android:layout_height="25dp"
    android:layout_marginBottom="2dp"
    android:layout_marginRight="0dp"
    android:text="Sets"
    android:textAlignment="textStart"
    android:textAppearance="@style/TextAppearance.AppCompat.Body1"
    android:textSize="24sp"
    app:layout_constraintBottom_toTopOf="@+id/listViewDay"
    app:layout_constraintRight_toLeftOf="@+id/guideline3"
    android:layout_marginLeft="0dp"
    app:layout_constraintLeft_toLeftOf="@+id/guideline2"
    app:layout_constraintHorizontal_bias="1.0"/>
  <TextView
    android:id="@+id/textView12"
    android:textStyle="bold"
    android:layout_width="44dp"
    android:layout_height="29dp"
    android:text="Pnds"
    android:textSize="24sp"
    tools:layout_editor_absoluteX="339dp"
    tools:layout_editor_absoluteY="102dp"/>


</android.support.constraint.ConstraintLayout>
